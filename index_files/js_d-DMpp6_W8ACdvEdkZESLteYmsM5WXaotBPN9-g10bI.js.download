    } else {
      console.log("Geolocation is not supported by this browser.");
    }
  }

  function writePositionToForm(position) {
    $('#events_newsletter_subscribe_form_latitude').val('Latitude', position.coords.latitude);
    $('#events_newsletter_subscribe_form_longitude').val('Longitude', position.coords.longitude);

    var latitude = position.coords.latitude;
    var longitude = position.coords.longitude;

    $.getJSON('/events/address/' + latitude + '/' + longitude, choseCountry(latitude, longitude));
  }

  function choseCountry(latitude, longitude) {
    return function (address) {
      var countryIso = address.country_iso;
      var cityName = address.city;
      $('select[name=select_country]').val(countryIso);
      $.getJSON('/events/cities/' + countryIso, addCities(cityName, countryIso, latitude, longitude));
    };

  }

  function addCities(cityName, countryIso, latitude, longitude) {
    return function(cities) {
      if (cities !== undefined && cities.length !== 0) {
        $('select[name=select_city]').html('<option value="City">City</option>');

        var wasCityChosen = false;

        $.each(cities, function (index, city) {
          var $option =  $('<option>').attr('value', city.name).text(city.name);

          if (cityName === city.name) {
            $option.attr('selected', 'selected');
            wasCityChosen = true;
          }
          $('select[name=select_city]').append($option);
        });

        if (wasCityChosen === false) {
          $.getJSON('/events/nearest_city/' + countryIso + '/' + latitude + '/' + longitude , choseNearestCity);
        }
      }
    };
  }

  function choseNearestCity(city) {
    $('select[name=select_city]').children().each(function (index, select_option) {
      if ($(select_option).val() == city.name) {
        $(this).attr('selected', 'selected');
      }
    });
  }

  function showError(error) {
    switch(error.code) {
      case error.PERMISSION_DENIED:
        console.log('User denied the request for Geolocation.');
        break;
      case error.POSITION_UNAVAILABLE:
        console.log('Location information is unavailable.');
        break;
      case error.TIMEOUT:
        console.log('The request to get user location timed out.');
        break;
      case error.UNKNOWN_ERROR:
        console.log('An unknown error occurred.');
        break;
    }
  }

  Drupal.behaviors.showLocationOfUser = {
    attach: function (context, settings) {
