 * @file
 *
 * Behavior for toggling mobile menu background.
 */

(function ($) {
  Drupal.behaviors.mobile_menu = {
    attach: function (context, settings) {
      $('.navbar-toggler', context).click(function () {
        var $navbar = $('.navbar');
        if ($navbar.hasClass('navbar-expand-mobile')) {
          setTimeout(function () {
            $('.navbar').removeClass('navbar-expand-mobile');
            $('body').removeClass('modal-open');
          }, 350);
        }
        else {
          $('.navbar').addClass('navbar-expand-mobile');
          $('body').addClass('modal-open');
        }
      });
    }
  };
})(jQuery);

