  /**
   * Checks windows scroll position and mark progress bar elements.
   *
   * @type {{attach: Drupal.behaviors.verticalProgressBar.attach}}
   */
  Drupal.behaviors.verticalProgressBar  = {
    attach: function (context) {
      $('.progress-navigation--vertical', context).once().each(function () {
        var $watchedProgressBar = $(this);
        var $progressBarItems = $watchedProgressBar.find('.progress-navigation__navigation');
        var $watchedTags = [];
        var animated = true;

        progressBarResizeMonitor();

        Drupal.onResize(progressBarResizeMonitor);

        $(window).scroll(function () {

          if (!Drupal.responsiveClasses.hasBreakpoint('lg')) {
            return;
          }

          var visibleOffsetTop = $(this).scrollTop();
          var visibleOffsetBottom = visibleOffsetTop + $(window).height() * 0.6;
          var selectedTagKey = null;

          for (var key in $watchedTags) {
            var $checkedItem = $watchedTags[key];
            var watchedItemBottomOffset = Math.ceil($checkedItem.offset().top) + $checkedItem.height() + 50;

            if (selectedTagKey === null &&
              visibleOffsetTop < watchedItemBottomOffset && watchedItemBottomOffset < visibleOffsetBottom ) {
              selectedTagKey = key;
            } else if (visibleOffsetBottom < watchedItemBottomOffset && selectedTagKey === null ) {
              selectedTagKey = key - 1;
            }
          }

          if (selectedTagKey === null) {
            return;
          }

          var $selectedViewContent = $watchedTags[selectedTagKey];

          if (!$selectedViewContent) {
            return;
          }

          if ($progressBarItems.prop('scrollHeight') + " " + $progressBarItems.height()) {
            // We know about vertical scrollbar for the progress bar items.

            // Let's find the target item to mark and check..
            var $targetItem = $watchedProgressBar.find('a[href="#' + $selectedViewContent.attr('id') + '"]');
            var targetItemRelativeOffsetTop = $targetItem.offset().top - $progressBarItems.offset().top;
            var scrollAmount = $progressBarItems.height() * 0.3;
            var targetAnimate = null;

            if ( scrollAmount > targetItemRelativeOffsetTop) {
              targetAnimate = $progressBarItems.scrollTop() - scrollAmount;
            } else if ( $progressBarItems.height() * 0.7 < targetItemRelativeOffsetTop) {
              targetAnimate = $progressBarItems.scrollTop() + scrollAmount;
            }

            if (targetAnimate && animated) {
              animated = false;
              $progressBarItems.stop().animate({
                scrollTop: targetAnimate
              }, 500, function() {
                animated = true;
              });
            }
          }

          if (!$selectedViewContent.is('.showing')) {
            for (var key in $watchedTags) {
              if (key != selectedTagKey) {
                $watchedTags[key].removeClass('showing');
              }
            }

            $selectedViewContent.addClass('showing');

            $watchedProgressBar.find('a[href="#' + $selectedViewContent.attr('id') + '"]').each(function () {
              var $visible = $(this).parents('.progress-indicator');
              $visible.addClass('marked selected');
              $visible.prevAll().addClass('marked').removeClass('selected');
              $visible.nextAll().removeClass('marked selected');
            });
          }

        }).scroll();

        function progressBarResizeMonitor() {
          $watchedProgressBar.find('.progress-indicator').once().each(function () {

            var $tagWithId = $(this).find('a[href]').first();
            if (!$tagWithId || $tagWithId.length <= 0) {
              return false;
            }
            $watchedTags[$watchedTags.length] = $($tagWithId.attr('href')).first();

          });
        }

      });
    }
  };

