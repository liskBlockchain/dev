/*global Drupal, jQuery, lottie */

(function ($, Drupal, lottie) {
  "use strict";

  Drupal.behaviors.lottie = {
    attach: function (context) {

      $(window).on('load', function () {
        $('.lottie-content__wrapper', context).each(function () {
          var $container = $(this);

          var path = Drupal.getAttribute($container, 'lottiePath', '');
          var params = {
            wrapper: this,
            renderer: 'svg',
            loop: true,
            autoplay: true,
            path: path
          };

          lottie.loadAnimation(params);
        });

        $('.media-banner--lottie-button .lottie-content__wrapper', context).click(function () {
          $(".media-banner .colorbox").click();
        });
      });
    }
  };
})(jQuery, Drupal, lottie);
