/**
 * @file
 * Global utilities.
 */

/*global Drupal, jQuery */

(function ($, Drupal) {

  'use strict';

  /**
   * Helper function to get data attribute or default value or all children count.
   *
   * @param $container
   *   jQuery dom element.
   * @param attribute
   *   Attribute name.
   * @param defaultValue
   *   Default value used if no attribute found.
   *
   * @returns {*}
   *   Returns element children count, if attribute equals "all".
   */
  Drupal.getAttribute = function ($container, attribute, defaultValue) {
    if ($container.data(attribute) === undefined) {
      return defaultValue;
    }

    var attributeValue = $container.data(attribute);
    if ('all' === attributeValue) {
      return $container.children().length;
    }

    return attributeValue;
  };

  Drupal.behaviors.bootstrap_barrio_subtheme = {
    attach: function () {
      $(window).scroll(function () {
        if ($(this).scrollTop() > 50) {
          $('body').addClass("scrolled");
        } else {
          $('body').removeClass("scrolled");
        }
      });

    }
  };

  Drupal.scrollTo = function ($target) {
    if ($target.length > 0) {
      var offset = Drupal.responsiveClasses.hasBreakpoint('md') ? 80 : 40;
      $('html, body').animate({
        scrollTop: $target.offset().top - offset
      }, 500);
    }
  };

  /**
   * Helper function to easly done thing after resizing.
   *
   * @param callback
   *   Callback to execute after resizing is finished.
   * @param interval
   *   How long the resize could take, to don't invoke callback.
   */
  Drupal.onResize = function (callback, interval) {
    var timeout;
    $(window).resize(function () {
      clearTimeout(timeout);
      timeout = setTimeout(callback, interval || 50);
    });
  };

  /**
   * Definition of responsive classes object.
   */
  Drupal.responsiveClasses = {
    classPrefix: 'window-width-',
    breakpoints: {
      'xs': 0,
      'sm': 576,
      'md': 768,
      'lg': 992,
      'xl': 1200,
    },
    resetClasses: function () {
      for (var i in this.breakpoints) {
        $('body').removeClass(this.classPrefix + i);
      }

      return this;
    },
    addClasses: function (breakpoints) {
      for (var i in breakpoints) {
        $('body').addClass(this.classPrefix + i);
      }

      return this;
    },
    filterBreakpoints: function () {
      var width = $(window).width();
      var newBreakpoints = $.extend({}, this.breakpoints);

      for (var i in newBreakpoints) {
        if (this.breakpoints[i] > width) {
          delete(newBreakpoints[i]);
        }
      }

      this
        .resetClasses()
        .addClasses(newBreakpoints);
    },
    hasBreakpoint: function (breakpoint) {
      return $('body').hasClass(this.classPrefix + breakpoint);
    }
  };

  /**
   * Helper object for easy on and off overlay.
   */
  Drupal.overlay = {
    activeClass: 'has-overlay',
    scope: function () {
      return $('body');
    },
    on: function () {
      this.scope().addClass(this.activeClass);
    },
    off: function () {
      this.scope().removeClass(this.activeClass);
    }
  };

  /**
   * Dynamically appends classes to body, depending on breakpoint.
   *
   * @type {{attach: Drupal.behaviors.breakpointDetect.attach}}
   */
  Drupal.behaviors.breakpointDetect = {
    attach: function () {
      Drupal.responsiveClasses.filterBreakpoints();
      Drupal.onResize(function () {
        Drupal.responsiveClasses.filterBreakpoints();
      });
      $(window).on('load', function () {
        Drupal.responsiveClasses.filterBreakpoints();
      });
    }
  };

  Drupal.behaviors.fixed_content = {
    attach: function () {
      Number.prototype.between = function (a, b) {
        var min = Math.min.apply(Math, [a, b]),
          max = Math.max.apply(Math, [a, b]);

        return this > min && this < max;
      };

      function makeBottomSticky(element) {
        element.addClass('bottom-sticked');
        element.removeClass('fixed').css('top', 'initial')
          .css('width', element.parent().outerWidth());
      }

      function makeFixed(element) {
        var fixedContentOffset = parseInt(element.data('offset-top'));

        element.removeClass('bottom-sticked');
        element.css('top', fixedContentOffset)
          .css('width', element.parent().outerWidth())
          .addClass('fixed');
      }

      function clearFixed(element) {
        element.removeClass('fixed')
          .css('top', 'initial')
          .css('width', 'initial');
        element.removeClass('bottom-sticked');
      }

      function getColumnOffsets(element) {
        var $fixedParent = element.parents('.fixed-content');

        var height = element.outerHeight();
        var top_offset = $fixedParent.offset().top;

        return {
          start: top_offset,
          height: height,
          stop: top_offset + $fixedParent.outerHeight() - element.outerHeight()
        };
      }

      function getElementSquareCoordinates(element) {
        return {
          top: parseInt(element.offset().top),
          left: parseInt(element.offset().left),
          bottom: parseInt(element.offset().top + element.outerHeight()),
          right: parseInt(element.offset().left + element.outerWidth())
        };
      }

      function detect($fixedContent, breakpoint) {
        var $scope = $fixedContent.closest($fixedContent.data('scope'));
        var fixedContentOffset = parseInt($fixedContent.data('offset-top'));

        var coliderSelector = $fixedContent.data('colider');
        var coliderOffsets = [];
        var $coliders = coliderSelector ? $scope.find(coliderSelector) : [];
        var $window = $(window);
        var offsets;

        function getColidersOffsets() {
          if (!$coliders.length) {
            return [];
          }

          var resultArray = [];
          var fixedElementCurrentCoordinates = getElementSquareCoordinates($fixedContent);

          $coliders.each(function () {
            var tempCoordinates = getElementSquareCoordinates($(this));

            if (fixedElementCurrentCoordinates.right.between(tempCoordinates.left, tempCoordinates.right) ||
              fixedElementCurrentCoordinates.left.between(tempCoordinates.left, tempCoordinates.right)) {
              resultArray[resultArray.length] = tempCoordinates;
            }
          });

          return resultArray;
        }

        function checkColisions() {
          if (!coliderOffsets.length) {
            $fixedContent.removeClass('fixed-faded');
            return false;
          }
          var currentCoordinates = getElementSquareCoordinates($fixedContent);

          for (var key in coliderOffsets) {
            var coliderInfo = coliderOffsets[key];

            if (currentCoordinates.top.between(coliderInfo.top, coliderInfo.bottom) ||
              currentCoordinates.bottom.between(coliderInfo.top, coliderInfo.bottom)) {
              if ($fixedContent.not('.fixed-faded')) {
                $fixedContent.addClass('fixed-faded');
              }
              return true;
            }
          }

          if ($fixedContent.is('.fixed-faded')) {
            $fixedContent.removeClass('fixed-faded');
          }

          return false;
        }

        $window.on("scroll", function () {
          // Additional checking to prevent scrolling.
          if (!Drupal.responsiveClasses.hasBreakpoint(breakpoint)) {
            return;
          }

          offsets = getColumnOffsets($fixedContent);
          coliderOffsets = getColidersOffsets();
          checkColisions();

          var currentScroll = $window.scrollTop() + fixedContentOffset;
          var currentBottom = offsets.stop;

          if (currentScroll >= currentBottom) {
            makeBottomSticky($fixedContent);
          }
          else if (currentScroll >= offsets.start) {
            makeFixed($fixedContent);
          }
          else {
            clearFixed($fixedContent);
          }

          // Additional reset for negative value, which sometimes appears on grid display mode.
          if (currentBottom < 0) {
            clearFixed($fixedContent);
          }
        });
      }

      $('.fixed-content__container').once().each(function () {
        var $fixedContent = $(this);
        var breakpoint = $fixedContent.data('breakpoint');

        if (Drupal.responsiveClasses.hasBreakpoint(breakpoint)) {
          detect($fixedContent, breakpoint);
        }

        Drupal.onResize(function () {
          if (Drupal.responsiveClasses.hasBreakpoint(breakpoint)) {
            var fixedContentOffset = parseInt($fixedContent.data('offset-top'));
            var offsets = getColumnOffsets($fixedContent);
            var currentScroll = $(window).scrollTop() + fixedContentOffset;

            // Make fixed on resize.
            if (currentScroll >= offsets.start && !$fixedContent.hasClass('bottom-sticked')) {
              makeFixed($fixedContent);
            }

            // Make sticky on resize.
            if (currentScroll >= offsets.stop) {
              makeBottomSticky($fixedContent);
            }

            // Scroll on resize.
            detect($fixedContent, breakpoint);
          }
          else {
            clearFixed($fixedContent);
          }
        }, 200);

      });
    }
  };

  /**
   * Adding simple fixed element support.
   *
   * @type {{attach: Drupal.behaviors.fixed_simply.attach}}
   */
  Drupal.behaviors.fixed_simply = {
    attach: function (context) {
      $('.fixed-when-reached:not(.js-inited)', context).each(function () {
        var $fixedContent = $(this);
        $fixedContent.addClass('js-inited').data("initpos", $fixedContent.offset().top);

        var $window = $(window);
        var contentOffsets;

        /**
         * Function returns sum of all fixed elements located above current.
         *
         * @returns {number}
         */
        function getPreviousFixedHeights() {
          var previousHeight = 0;

          $('body').find('.fixed-when-reached.fixed').each(function () {
            if ($(this).data('initpos') < $fixedContent.data('initpos')) {
              previousHeight += $(this).height();
            }
          });

          return previousHeight;
        }

        $window.on("resize", function () {
          contentOffsets = $fixedContent.offset();
          $fixedContent.removeClass('fixed').css('top', 'initial');
        }).resize();

        $window.on("scroll", function () {
          var currentScroll = $window.scrollTop();
          var visibleFixedHeight = getPreviousFixedHeights($fixedContent);
          if (currentScroll + visibleFixedHeight >= contentOffsets.top) {
            $fixedContent.css('top', visibleFixedHeight).addClass('fixed');
          } else {
            $fixedContent.removeClass('fixed').css('top', 'initial');
          }
        });
      });
    }
  };

  Drupal.behaviors.slick_init_default = {
    attach: function (context) {
      function addSliderTo($slickContainer) {
        /* Get current container data attributes to use them form breakpoints settings. */
        var xsCenter = Drupal.getAttribute($slickContainer, 'sliderCenterXs', false);
        var smCenter = Drupal.getAttribute($slickContainer, 'sliderCenterSm', false);
        var centerPadding = Drupal.getAttribute($slickContainer, 'sliderCenterPadding', 0);
        var variableWidth = Drupal.getAttribute($slickContainer, 'sliderVariableWidth', true);
        var xsSlidesVisible = Drupal.getAttribute($slickContainer, 'sliderItemsVisibleXs', 1);
        var mdSlidesVisible = Drupal.getAttribute($slickContainer, 'sliderItemsVisibleMd', 2 * xsSlidesVisible);
        var lgSlidesVisible = Drupal.getAttribute($slickContainer, 'sliderItemsVisibleLg', 3 * xsSlidesVisible);

        $slickContainer.not('.slick-initialized').slick({
          centerMode: xsCenter,
          centerPadding: centerPadding,
          variableWidth: variableWidth,
          dots: false,
          mobileFirst: true,
          infinite: false,
          arrows: false,
          autoplay: false,
          speed: 300,
          slidesToShow: xsSlidesVisible,
          slidesToScroll: 1,
          adaptiveHeight: false,
          responsive: [
            {
              breakpoint: 576,
              settings: {
                centerMode: smCenter
              }
            },
            {
              breakpoint: 768,
              settings: {
                slidesToShow: mdSlidesVisible,
                centerMode: smCenter
              }
            },
            {
              breakpoint: 992,
              settings: {
                slidesToShow: lgSlidesVisible,
                centerMode: smCenter
              }
            },
            {
              breakpoint: 1200,
              settings: "unslick"
            }
          ]
        });
      }

      $('.slick-slider-container:not(.js-inited)', context).each(function () {
        var $slickContainer = $(this);
        $slickContainer.addClass('js-inited');

        /* Let's attach slider to current element. */
        addSliderTo($slickContainer);
      });

      $(window).on("resize", function () {
        var windowWidth = $(window).width();

        /* Currently, slider is detached/unslicked on big screens.
         * If users screen'll get downsized we should attach slider again.
         */
        if (windowWidth < 1200) {
          $('.slick-slider-container.js-inited', context).each(function () {
            addSliderTo($(this));
          });
        }
      });
    }
  };

  /**
   * This is solving problem with IOS and hover effect staying after touch.
   *
   * @type {{attach: Drupal.behaviors.hovering_overflow.attach}}
   */
  Drupal.behaviors.hovering_overflow = {
    attach: function (context) {
      var isTouch = !!("ontouchstart" in window) || window.navigator.msMaxTouchPoints > 0;

      if (!isTouch) {
        $('.hover-dependant', context).addClass('hover-safe');
      }
    }
  };

  /**
   * Adding js automatic smooth scroll support.
   *
   * @type {{attach: Drupal.behaviors.autoSmoothScrolling.attach}}
   */
  Drupal.behaviors.smoothscrolling = {
    attach: function (context) {
      $('.smoothly-scroll-to', context).once().each(function () {

        $(this).find('a').on('click', function (e) {
          var $link = $(this);
          /* Get current link href attribute and search for #fragment. */
          var linkHref = $link.attr('href');
          var targetElementId = linkHref.substr(linkHref.lastIndexOf('#'));
          /* If on current page we find element matched by requested id, we smooth scroll to it.*/
          var $targetElement = $(targetElementId);

          if ($targetElement.length > 0) {
            Drupal.scrollTo($targetElement);
            e.preventDefault();
          }
        });
      });

    }
  };

  /**
   * Use selectboxit for all select items.
   *
   * @type {{attach: Drupal.behaviors.selectBoxIt.attach}}
   */
  Drupal.behaviors.selectBoxIt = {
    attach: function (context, settings) {
      if (typeof $.fn.selectBoxIt == "undefined") {
        return;
      }

      $('select', context).selectBoxIt({
        downArrowIcon: 'fas fa-chevron-down'
      });
    }
  };

  /**
   * Toggle open class for accordion.
   *
   * @type {{attach: Drupal.behaviors.accordion.attach}}
   */
  Drupal.behaviors.accordion = {
    attach: function (context, settings) {
      var selector = '[data-toggle="collapse"]';

      $('.accordion', context).on('show.bs.collapse hide.bs.collapse', function (e) {
        var accordion = $(this);
        var label = accordion.find('[data-label-expanded]');

        accordion.toggleClass('open');
        if (label.length < 1) {
          return;
        }
        var text = e.type == "hide" ? label.data('label-collapsed') : label.data('label-expanded');
        label.text(text);
      });

      $('[data-toggle-target]').once().click(function (e) {
        e.preventDefault();
        var target = $(this).data('toggle-target');
        $(selector + '[href="#' + target + '"]').trigger('click');
      });
    }
  };

  /**
   * Adding js for expanding nearest hidden block.
   *
   * @type {{attach: Drupal.behaviors.expandNearest.attach}}
   */
  Drupal.behaviors.expandNearest = {
    attach: function (context) {
      $('a.expand-nearest-block', context).each(function () {
        var $link = $(this);
        $link.on('click', function () {
          $link.parents('.nearest-restriction').find('.expandable-block').collapse('toggle');
          $link.toggleClass('showing-more');
          return false;
        });
      });
    }
  };

  /**
   * Provides functionality to retrieve selector from url hash and show + scroll to the item.
   *
   * @type {{attach: Drupal.behaviors.fragmentTarget.attach}}
   */
  Drupal.behaviors.fragmentTarget = {
    attach: function (context, settings) {
      $(window).on('load', function () {
        hashExpander(window.location.hash.split('#'));
      });

      // anjuna: also trigger hashclicks in footer while on /faq (not just on load ^)
      if ($( "body" ).hasClass( "path-faq" )) {
        $(".main-footer").find("a[href*='faq#']").on('click', function (e) {
          e.preventDefault();
          hashExpander(e.target.hash.split('#'));
        });
      }

      function hashExpander(hash) {
        if (hash.length < 1) {
          return false;
        }
        var $target = $('[data-fragment-target="' + hash[1] + '"]').eq(0);
        if ($target.length < 1) {
          return false;
        }
        var $accordion = $target.closest('.accordion');
        var $restriction = $target.closest('.nearest-restriction');

        if ($accordion.is(':hidden')) {
          $restriction.find(".expand-nearest-block").click();
        }
        $accordion.find('[data-toggle][aria-expanded="false"]').click();

        Drupal.scrollTo($target);
      }

    }
  };

  /**
   * This detects the OS and adds it to the <body> tag
   *
   * @type {{attach: Drupal.behaviors.fragmentTarget.attach}}
   */
  Drupal.behaviors.osDetect = {
    attach: function () {
      function showFirstCTA($class) {
        $($class).each(function () {
          var $wrapper = $(this);
          var countVisibleDependantItems = $wrapper.find('.cta-os:visible').length;
          if (countVisibleDependantItems <= 0) {
            $wrapper.find('.cta-os:first').show();
          }
        });
      }

      function osDetect() {
        var OSName = "unknownos";

        OSName = navigator.platform.indexOf("Win") !== -1 ? "windows" : OSName;
        OSName = navigator.platform.indexOf("Mac") !== -1 ? "macos" : OSName;
        OSName = navigator.platform.indexOf("Mac") !== -1 ? "macos" : OSName;
        OSName = navigator.platform.indexOf("X11") !== -1 || navigator.platform.indexOf("Linux x") !== -1 ? "linux" : OSName;
        OSName = navigator.platform.indexOf("iPhone") !== -1 || navigator.platform.indexOf("iPad") !== -1 ? "ios" : OSName;
        OSName = navigator.platform.indexOf("Android") !== -1 || navigator.platform.indexOf("Linux arm") !== -1 ||
        navigator.platform.indexOf("Linux aarch") !== -1 ? "android" : OSName;

        $('html').addClass(OSName);

        var windowWidth = $(window).width();

        if (windowWidth >= 992) {
          showFirstCTA('.os-dependant-desktop-wrapper');
        }

        showFirstCTA('.os-dependant-wrapper');
      }

      Drupal.onResize(function () {
        osDetect();
      }, 200);

      $(window).on('load', function () {
        osDetect();
      });
    }
  };

  /**
   * Adding js automatic smooth scroll to tab && pills.
   *
   * @type {{attach: Drupal.behaviors.scrollToTab.attach}}
   */
  Drupal.behaviors.scrollToTab = {
    attach: function (context) {
      $('[data-toggle="pill"][data-scrollto]', context).on('shown.bs.tab', function () {
        if (Drupal.responsiveClasses.hasBreakpoint('lg')) {
          return;
        }
        var target = $(this).data('target');
        Drupal.scrollTo($(target));
      });
    }
  };

  /**
   * Allow display person's panels as collapsing modal.
   *
   * @type {{attach: Drupal.behaviors.personsToggleApp.attach}}
   */
  Drupal.behaviors.personsToggleApp = {
    attach: function (context) {
      var app = $('[data-person-toogle-app]');
